<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.haibo.base.mapper.UserMapper">
	<insert id="insertUser" parameterType="com.haibo.base.entity.User">
		INSERT INTO mysql_user(user_id,user_name,password) VALUES (#{userName},#{userId},#{password})
	</insert>
</mapper>
