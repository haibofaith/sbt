<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.haibo.base.mapper.TransMapper">
	<select id="selectUser" resultType="com.haibo.base.entity.User">
		select id,name,sex from t_2
	</select>
	<insert id="insertUser">
		insert into t_2(id,name,sex) values (#{user.id},#{user.name},#{user.sex})
	</insert>
</mapper>
